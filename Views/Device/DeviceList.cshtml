                                { text: '添加', onclick: function () { deviceOut_add(); } },
                                { text: '取消', onclick: function () { addDeviceOutDialog.hide(); } }
                            ]
                        });
                    }
                    break
                case "deviceStorageInfo":

                    if (storageDialog) {
                        storageDialog.show();
                    }
                    else {
                        storageDialog = $.ligerDialog.open({
                            target: $("#storageForm"), title: '全部商品的高低储状态',
