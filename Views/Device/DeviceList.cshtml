                        DeviceID: data.ID,
                        DeviceName: data.Name,
                        Behavior: "出库",
                        Mode: data.Mode
                    });


                    if (addDeviceOutDialog) {
                        addDeviceOutDialog.show();
                    }
                    else {
                        addDeviceOutDialog = $.ligerDialog.open({
                            target: $("#AddDeviceOutForms"), title: '出库',
                            width: 450, height: 320, top: 70, left: 280,  // 弹出窗口大小
